{
    "name": "sitrunlab/learn-kahlan",
    "description": "learn kahlan",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.1.3",
        "kahlan/extra-matcher": "dev-master",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "suggest": {
        "ext-xdebug": "Xdebug extension to allow to get coverage report",
        "ext-phpdbg": "PHPDBG extension to allow to get coverage report"
    }
}
